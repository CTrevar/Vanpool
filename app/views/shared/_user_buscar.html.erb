<div class="box">
                        <div class="box-header">
                        </br>
                          <h3 class="box-title">Busca tu ruta</h3>
                        </div><!-- /.box-header -->
                            <!-- form start -->
                              <form role="form">
                                <div class="box-body">
                                  
                                    <div class="span6">
                                      <div class="form-group">
                                        <label for="origen">¿Dónde comienzas tu viaje?</label>
                                        <div class="input-group">
                                          <span class="input-group-addon"><i class="fa fa-map-marker"></i></span>
                                          <input id="origenRuta" class="form-control"  placeholder="Origen">
                                        </div>
                                    </div>                  
                                  </div>

                                  <div class="span6">
                                  <div class="form-group">
                                    <label for="destino">¿Dónde terminas tu viaje?</label>
                                      <div class="input-group">
                                        <span class="input-group-addon"><i class="fa fa-map-marker"></i></span>
                                        <input id="destinoRuta" class="form-control"  placeholder="Destino">
                                      </div>
                                    </div>
                                  </div>
                                  <div class="form-group">
                                    <label for="diaViaje">¿Cuándo quieres viajar?</label>
                                        <div class="row-fluid">
                                          <div class="span6">
                                            <div class="form-group">
                                            <div class="input-group">
                                            <div class="input-group-addon">
                                                <i class="fa fa-calendar"></i>
                                            </div>
                                            <input  id="diaViajeRuta" type="text" class="form-control" data-inputmask="'alias': 'dd/mm/yyyy'" data-mask/>
                                        </div><!-- /.input group -->
                                    </div><!-- /.form group -->
                                          </div>
                                          <div class="span6">
                                           <div class="bootstrap-timepicker">
                                            
                                        <div class="form-group">
                                          
                                            <div class="input-group">
                                                <div class="input-group-addon">
                                                    <i class="fa fa-clock-o"></i>
                                                </div>
                                                <input type="text" id="horarioRuta" class="form-control timepicker"/>
                                            </div><!-- /.input group -->
                                        </div><!-- /.form group -->
                                    </div>
                                  </div>
                                        </div>
                                  </div>
                                    <button type="submit" class="btn btn-flat btn-buscar pull-right">Buscar mi ruta</button>

                                </div><!-- /.box-body -->
                                  </br>
                                      </br>
                                    </br>
                              </form>
                    </div><!-- /.box -->